<?xml version="1.0"?>
<robot name="junior">
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <material name="skyblue">
        <color rgba="0 0.75 1 1"/>
    </material>

    <material name="silver">
        <color rgba="0.77 0.78 0.78 1"/>
    </material>

    <!-- Base Link -->
    <link name="base_link">
        
    </link>

    <!-- Chassis Link -->
    <link name="Chassis">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitChassisVisual.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitChassis.stl"/>
            </geometry>
            <material name="red"/>
        </collision>
        <inertial>
            <mass value="45"/>
            <inertia ixx="1.206" ixy="0.0" ixz="0.0" iyy="2.182" iyz="0.0" izz="2.182"/>
        </inertial>
    </link>

    <!-- Front Bumper Link -->
    <link name="FrontBumper">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontBumper.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontBumper.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>


    <!-- Back Bumper Link -->
    <link name="BackBumper">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackBumper.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackBumper.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="0.014" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>


    <!-- Front Lidar Link -->
    <link name="FrontLidar">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarCase.stl"/>
            </geometry>
            <material name="skyblue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarCase.stl"/>
            </geometry>
            <material name="skyblue"/>
        </collision>
        <inertial>
            <mass value="2"/>
            <inertia ixx="0.009" ixy="0.0" ixz="0.0" iyy="0.009" iyz="0.0" izz="0.009"/>
        </inertial>
    </link>

    <!-- Front Lidar Glass Link -->
    <link name="FrontLidarGlass">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarGlass.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarGlass.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <!-- Back Camera Case Link -->
    <link name="BackCameraCase">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackCameraCase.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackCameraCase.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="0.5"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <!-- Back Camera Stand Link -->
    <link name="BackCameraStand">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackCameraStand.stl"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackCameraStand.stl"/>
            </geometry>
            <material name="silver"/>
        </collision>
        <inertial>
            <mass value="0.5"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <!-- Back Camera Head Link -->
    <link name="BackCameraHead">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackCameraHead.stl"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackCameraHead.stl"/>
            </geometry>
            <material name="silver"/>
        </collision>
        <inertial>
            <mass value="0.5"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <!-- Risers Link -->
    <link name="Risers">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitRiserVisual.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitRiserCollision.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="0.5"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <!-- Left Front Link -->
    <link name="WheelLeftFront">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <!-- <mesh filename="file://$(find junior_description)/meshes/robitWheelCollision.dae"/> -->
                <cylinder length="0.105" radius="0.1575"/>
            </geometry>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
            <material name="black"/>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                        <kp>10000000.0</kp>
                        <kd>1.0</kd>
                        <fdir>1 0 0</fdir>
                    </ode>
                </friction>
            </surface>
        </collision>
        <inertial>
            <mass value="2"/>
            <inertia ixx="0.014240625" ixy="0.0" ixz="0.0" iyy="0.014240625" iyz="0.0" izz="0.02480625"/>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
        </inertial>
    </link>

    <!-- Right Front Link -->
    <link name="WheelRightFront">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <!-- <mesh filename="file://$(find junior_description)/meshes/robitWheelCollision.dae"/> -->
                <cylinder length="0.105" radius="0.1575"/>
            </geometry>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
            <material name="black"/>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                        <kp>10000000.0</kp>
                        <kd>1.0</kd>
                        <fdir>1 0 0</fdir>
                    </ode>
                </friction>
            </surface>
        </collision>
        <inertial>
            <mass value="2"/>
            <inertia ixx="0.014240625" ixy="0.0" ixz="0.0" iyy="0.014240625" iyz="0.0" izz="0.02480625"/>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
        </inertial>
    </link>

    <!-- Left Back Link -->
    <link name="WheelLeftBack">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <!-- <mesh filename="file://$(find junior_description)/meshes/robitWheelCollision.dae"/> -->
                <cylinder length="0.105" radius="0.1575"/>
            </geometry>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
            <material name="black"/>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                        <kp>10000000.0</kp>
                        <kd>1.0</kd>
                        <fdir>1 0 0</fdir>
                    </ode>
                </friction>
            </surface>
        </collision>
        <inertial>
            <mass value="2"/>
            <inertia ixx="0.014240625" ixy="0.0" ixz="0.0" iyy="0.014240625" iyz="0.0" izz="0.02480625"/>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
        </inertial>
    </link>

    <!-- Right Back Link -->
    <link name="WheelRightBack">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <!-- <mesh filename="file://$(find junior_description)/meshes/robitWheelCollision.dae"/> -->
                <cylinder length="0.105" radius="0.1575"/>
            </geometry>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
            <material name="black"/>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                        <kp>10000000.0</kp>
                        <kd>1.0</kd>
                        <fdir>1 0 0</fdir>
                    </ode>
                </friction>
            </surface>
        </collision>
        <inertial>
            <mass value="2"/>
            <inertia ixx="0.014240625" ixy="0.0" ixz="0.0" iyy="0.014240625" iyz="0.0" izz="0.02480625"/>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
        </inertial>
    </link>

    <!-- Graveyard of Wheel Inertias
    
    Ted v1 : <inertia ixx="0.007" ixy="0.0" ixz="0.0" iyy="0.014" iyz="0.0" izz="0.007"/>
    Cylinder Math : <inertia ixx="0.014" ixy="0.0" ixz="0.0" iyy="0.014" iyz="0.0" izz="0.024"/>
    Box Math : <inertia ixx="0.0183" ixy="0.0" ixz="0.0" iyy="0.033" iyz="0.0" izz="0.033"/>
    Logi v1 : <inertia ixx="0.014" ixy="0.0" ixz="0.0" iyy="0.04" iyz="0.0" izz="0.024"/>
    CURRENT: Precision Cylinder : <inertia ixx="0.014240625" ixy="0.0" ixz="0.0" iyy="0.014240625" iyz="0.0" izz="0.02480625"/>
    Gazebo Cylinder : <inertia ixx="0.15" ixy="0.0" ixz="0.0" iyy="0.15" iyz="0.0" izz="0.13"/>
    
    -->

    <!-- START OF JOINTS -->
    <joint name="ChassisJoint" type="fixed">
        <parent link="base_link"/>
        <child link="Chassis"/>
        <origin xyz="0 0 0.23"/>
    </joint>

    <joint name="FrontLidarHolster" type="fixed">
        <parent link="Chassis"/>
        <child link="FrontLidar"/>
        <origin xyz="0.321 0 -0.0197"/> 
    </joint>

    <joint name="FrontLidarVision" type="fixed">
        <parent link="FrontLidar"/>
        <child link="FrontLidarGlass"/>
        <origin xyz="0.075 0 0"/> 
    </joint>

    <joint name="BackCameraHolster" type="fixed">
        <parent link="Chassis"/>
        <child link="BackCameraCase"/>
        <origin xyz="-0.285 0 0.339"/>
    </joint>

    <joint name="BackCameraStandJoint" type="fixed">
        <parent link="BackCameraCase"/>
        <child link="BackCameraStand"/>
        <origin xyz="0 0 -0.045"/>
        <axis xyz="0 0 1"/>
        <!-- <limit lower="-1" upper="1" effort="5" velocity="2"/>  FOR REVOLUTE -->
    </joint>

    <joint name="BackCameraHeadJoint" type="fixed">
        <parent link="BackCameraStand"/>
        <child link="BackCameraHead"/>
        <origin xyz="0 0 0.05" rpy="0 -3.14 0"/>
        <axis xyz="0 1 0"/>
        <!-- <dynamics damping="0" friction="0.75"/>
        <limit lower="-1" upper="1" effort="5" velocity="2"/> FOR REVOLUTE -->
    </joint>

    <joint name="RisersJoint" type="fixed">
        <parent link="Chassis"/>
        <child link="Risers"/>
        <origin xyz="-0.057 0 0.2655"/>
    </joint>

    <joint name="AxleLeftFront" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelLeftFront"/>
        <origin xyz="0.193 0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>
    
    <joint name="AxleRightFront" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelRightFront"/>
        <origin xyz="0.193 -0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="AxleLeftBack" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelLeftBack"/>
        <origin xyz="-0.193 0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="AxleRightBack" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelRightBack"/>
        <origin xyz="-0.193 -0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="FrontBumperLoc" type="fixed">
        <parent link="Chassis"/>
        <child link="FrontBumper"/>
        <origin xyz="0.321 0 0.025" rpy="0 0 0"/>
    </joint>

    <joint name="BackBumperLoc" type="fixed">
        <parent link="Chassis"/>
        <child link="BackBumper"/>
        <origin xyz="-0.321 0 -0.0795" rpy="0 0 0"/>
    </joint>
    <!-- END OF JOINTS -->


    <!-- START OF GAZEBO -->
    <!-- Chassis -->
    <gazebo reference="Chassis">
    </gazebo>

    <!-- Front Bumper -->
    <gazebo reference="FrontBumper">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Bumper -->
    <gazebo reference="BackBumper">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Front Lidar Case -->
    <gazebo reference="FrontLidar">
        <visual>
            <material>
                <diffuse>0.1 0.5 1 1</diffuse>
                <ambient>0.0666 0.333 0.4 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Front Lidar -->
    <gazebo reference="FrontLidarGlass">
        <sensor name="laser" type="ray">
            <pose> 0 0 0 0 0 0 </pose>
            <visualize>true</visualize>
            <update_rate>10</update_rate>
            <ray>
                <scan>
                    <horizontal>
                    <samples>360</samples>
                    <min_angle>-1.57</min_angle>
                    <max_angle>1.57</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.2</min>
                    <max>3</max>
                </range>
            </ray>
            <plugin name="fl_controller" filename="libgazebo_ros_ray_sensor.so">
                <ros>
                    <argument>-/out:=scan</argument>
                </ros>
                <output_type>sensor_msgs/LaserScan</output_type>
                <frame_name>FrontLidarGlass</frame_name>
            </plugin>
        </sensor>
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Camera Case -->
    <gazebo reference="BackCameraCase">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Lidar Stand -->
    <gazebo reference="BackCameraStand">
        <visual>
            <material>
                <diffuse>0.77 0.78 0.78 1</diffuse>
                <ambient>0.385 0.39 0.39 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Lidar Head -->
    <gazebo reference="BackCameraHead">
        <sensor name="blaser" type="ray">
            <pose> 0 0 0 0 0 </pose>
            <visualize>true</visualize>
            <update_rate>10</update_rate>
            <ray>
                <scan>
                    <horizontal>
                    <samples>120</samples>
                    <min_angle>-0.52</min_angle>
                    <max_angle>0.52</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.1</min>
                    <max>2</max>
                </range>
            </ray>
            <plugin name="bl_controller" filename="libgazebo_ros_ray_sensor.so">
                <ros>
                    <argument>-/out:=scan</argument>
                </ros>
                <output_type>sensor_msgs/LaserScan</output_type>
                <frame_name>BackCameraHead</frame_name>
            </plugin>
        </sensor>
        <visual>
            <material>
                <diffuse>0.77 0.78 0.78 1</diffuse>
                <ambient>0.385 0.39 0.39 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Risers -->
    <gazebo reference="Risers">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- WHEELS -->
    <gazebo reference="WheelLeftFront">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <gazebo reference="WheelRightFront">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <gazebo reference="WheelLeftBack">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <gazebo reference="WheelRightBack">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>
    <!-- END OF WHEELS -->

    <gazebo>
        <plugin name="wheel_drive_controller" filename="libgazebo_ros_diff_drive.so">
            <!-- wheels -->
            <num_wheel_pairs>2</num_wheel_pairs>
            <left_joint>AxleLeftFront</left_joint>
            <left_joint>AxleLeftBack</left_joint>
            <right_joint>AxleRightFront</right_joint>
            <right_joint>AxleRightBack</right_joint>

            <!-- kinematics -->
            <wheel_separation>0.435</wheel_separation>
            <wheel_diameter>0.315</wheel_diameter>

            <!-- Limits -->
            <max_wheel_torque>30.0</max_wheel_torque>
            <max_wheel_acceleration>3.0</max_wheel_acceleration>

            <!-- Output -->
            <odometry_frame>odom</odometry_frame>
            <robot_base_frame>base_link</robot_base_frame>

            <!-- topic & re remappings -->
            <!-- ros>
                <namespace>/</namespace>
                <argument>/cmd_vel:=cmd_vel</argument>
                <argument>/odom:=odom</argument>
            </ros -->

            <publish_odom>true</publish_odom>
            <publish_odom_tf>true</publish_odom_tf>
            <update_rate>30.0</update_rate>
        </plugin>
    </gazebo>
    <!-- END OF GAZEBO -->
</robot>