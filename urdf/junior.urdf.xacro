<?xml version="1.0"?>
<robot name="junior">
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <material name="skyblue">
        <color rgba="0 0.75 1 1"/>
    </material>

    <material name="silver">
        <color rgba="0.77 0.78 0.78 1"/>
    </material>

    <!-- Base Link -->
    <link name="base_link">
        
    </link>

    <!-- Chassis Link -->
    <link name="Chassis">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitChassisVisual.stl"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitChassis.stl"/>
            </geometry>
            <material name="red"/>
        </collision>
        <inertial>
            <mass value="35"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Front Bumper Link -->
    <link name="FrontBumper">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontBumper.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontBumper.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="3"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>


    <!-- Back Bumper Link -->
    <link name="BackBumper">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackBumper.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackBumper.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="1.5"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>


    <!-- Front Lidar Link -->
    <link name="FrontLidar">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarCase.stl"/>
            </geometry>
            <material name="skyblue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarCase.stl"/>
            </geometry>
            <material name="skyblue"/>
        </collision>
        <inertial>
            <mass value="2"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Front Lidar Glass Link -->
    <link name="FrontLidarGlass">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarGlass.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitFrontLidarGlass.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Back Lidar Case Link -->
    <link name="BackLidarCase">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackLidarCase.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackLidarCase.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="2"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Back Lidar Stand Link -->
    <link name="BackLidarStand">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackLidarStand.stl"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackLidarStand.stl"/>
            </geometry>
            <material name="silver"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Back Lidar Head -->
    <link name="BackLidarHead">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackLidarHead.stl"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitBackLidarHead.stl"/>
            </geometry>
            <material name="silver"/>
        </collision>
        <inertial>
            <mass value="5"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Left Front Link -->
    <link name="WheelLeftFront">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="5"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Right Front Link -->
    <link name="WheelRightFront">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="5"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Left Back Link -->
    <link name="WheelLeftBack">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="5"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>

    <!-- Right Back Link -->
    <link name="WheelRightBack">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_description)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </collision>
        <inertial>
            <mass value="5"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
        </inertial>
    </link>


    <!-- START OF JOINTS -->
    <joint name="ChassisJoint" type="fixed">
        <parent link="base_link"/>
        <child link="Chassis"/>
        <origin xyz="0 0 0.23"/>
    </joint>

    <joint name="FrontLidarHolster" type="fixed">
        <parent link="Chassis"/>
        <child link="FrontLidar"/>
        <origin xyz="0.321 0 -0.0197"/> 
    </joint>

    <joint name="FrontLidarVision" type="fixed">
        <parent link="FrontLidar"/>
        <child link="FrontLidarGlass"/>
        <origin xyz="0.075 0 0"/> 
    </joint>

    <joint name="BackLidarHolster" type="fixed">
        <parent link="Chassis"/>
        <child link="BackLidarCase"/>
        <origin xyz="-0.285 0 0.319"/>
    </joint>

    <joint name="BackLidarStandJoint" type="revolute">
        <parent link="BackLidarCase"/>
        <child link="BackLidarStand"/>
        <origin xyz="0 0 -0.045"/>
        <axis xyz="0 0 1"/>
        <limit lower="-1" upper="1" effort="5" velocity="2"/>
    </joint>

    <joint name="BackLidarHeadJoint" type="revolute">
        <parent link="BackLidarStand"/>
        <child link="BackLidarHead"/>
        <origin xyz="0 0 0.05" rpy="0 -3.14 0"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="0" friction="0.75"/>
        <limit lower="-1" upper="1" effort="5" velocity="2"/>
    </joint>

    <joint name="AxleLeftFront" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelLeftFront"/>
        <origin xyz="0.193 0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>
    
    <joint name="AxleRightFront" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelRightFront"/>
        <origin xyz="0.193 -0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="AxleLeftBack" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelLeftBack"/>
        <origin xyz="-0.193 0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="AxleRightBack" type="continuous">
        <parent link="Chassis"/>
        <child link="WheelRightBack"/>
        <origin xyz="-0.193 -0.2635 -0.07" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="FrontBumperLoc" type="fixed">
        <parent link="Chassis"/>
        <child link="FrontBumper"/>
        <origin xyz="0.321 0 0.025" rpy="0 0 0"/>
    </joint>

    <joint name="BackBumperLoc" type="fixed">
        <parent link="Chassis"/>
        <child link="BackBumper"/>
        <origin xyz="-0.321 0 -0.0795" rpy="0 0 0"/>
    </joint>
    <!-- END OF JOINTS -->


    <!-- START OF GAZEBO -->
    <!-- Chassis -->
    <gazebo reference="Chassis">
        <visual>
            <material>
                <diffuse>1 0 0 1</diffuse>
                <ambient>0.4 0.05 0.05 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Front Bumper -->
    <gazebo reference="FrontBumper">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Bumper -->
    <gazebo reference="BackBumper">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Front Lidar Case -->
    <gazebo reference="FrontLidar">
        <visual>
            <material>
                <diffuse>0.1 0.5 1 1</diffuse>
                <ambient>0.0666 0.333 0.4 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Front Lidar -->
    <gazebo reference="FrontLidarGlass">
        <sensor name="laser" type="ray">
            <pose> 0 0 0 0 0 0 </pose>
            <visualize>true</visualize>
            <update_rate>10</update_rate>
            <ray>
                <scan>
                    <horizontal>
                    <samples>360</samples>
                    <min_angle>-1.57</min_angle>
                    <max_angle>1.57</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.2</min>
                    <max>3</max>
                </range>
            </ray>
            <plugin name="fl_controller" filename="libgazebo_ros_ray_sensor.so">
                <ros>
                    <argument>-/out:=scan</argument>
                </ros>
                <output_type>sensor_msgs/LaserScan</output_type>
                <frame_name>FrontLidarGlass</frame_name>
            </plugin>
        </sensor>
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Lidar Case -->
    <gazebo reference="BackLidarCase">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Lidar Stand -->
    <gazebo reference="BackLidarStand">
        <visual>
            <material>
                <diffuse>0.77 0.78 0.78 1</diffuse>
                <ambient>0.385 0.39 0.39 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Back Lidar Head -->
    <gazebo reference="BackLidarHead">
        <sensor name="blaser" type="ray">
            <pose> 0 0 0 0 0 </pose>
            <visualize>true</visualize>
            <update_rate>10</update_rate>
            <ray>
                <scan>
                    <horizontal>
                    <samples>120</samples>
                    <min_angle>-0.52</min_angle>
                    <max_angle>0.52</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.1</min>
                    <max>2</max>
                </range>
            </ray>
            <plugin name="bl_controller" filename="libgazebo_ros_ray_sensor.so">
                <ros>
                    <argument>-/out:=scan</argument>
                </ros>
                <output_type>sensor_msgs/LaserScan</output_type>
                <frame_name>BackLidarHead</frame_name>
            </plugin>
        </sensor>
        <visual>
            <material>
                <diffuse>0.77 0.78 0.78 1</diffuse>
                <ambient>0.385 0.39 0.39 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- WHEELS -->
    <gazebo reference="WheelLeftFront">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <gazebo reference="WheelRightFront">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <gazebo reference="WheelLeftBack">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>

    <gazebo reference="WheelRightBack">
        <visual>
            <material>
                <diffuse>0.2 0.2 0.2 1</diffuse>
                <ambient>0.1 0.1 0.1 1</ambient>
                <emissive>0 0 0 0</emissive>
            </material>
        </visual>
    </gazebo>
    <!-- END OF WHEELS -->
    <!-- END OF GAZEBO -->
</robot>